{
  "name": "flush-s3-bucket",
  "version": "0.0.0-development",
  "description": "Delete all files in an S3 bucket.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Mathis Wiehl (@fahrradflucht) <mail@mathiswiehl.de>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fahrradflucht/flush-s3-bucket.git"
  },
  "keywords": [
    "empty",
    "aws"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.{js,json,md,ts}'",
    "format:verify": "prettier --list-different '**/*.{js,json,md,ts}'",
    "lint": "tslint --project .",
    "test": "jest"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "7.1.2",
    "@commitlint/travis-cli": "7.2.1",
    "@types/async": "2.0.50",
    "@types/jest": "23.3.10",
    "@types/node": "10.12.11",
    "babel-core": "6.26.3",
    "commitlint": "7.2.1",
    "husky": "1.2.0",
    "jest": "23.6.0",
    "prettier": "1.15.3",
    "semantic-release": "15.12.3",
    "ts-config": "20.7.0",
    "ts-jest": "23.10.5",
    "tslint": "5.11.0",
    "typescript": "3.1.6"
  },
  "dependencies": {
    "async": "^2.6.0",
    "aws-sdk": "^2.218.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
