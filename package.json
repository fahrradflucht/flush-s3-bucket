    "lint": "tslint --project .",
    "test": "jest"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "8.3.4",
    "@commitlint/travis-cli": "8.3.5",
    "@types/jest": "25.1.3",
    "@types/node": "10.12.24",
    "babel-core": "6.26.3",
    "commitlint": "8.3.5",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "semantic-release": "15.14.0",
    "ts-config": "20.10.0",
    "ts-jest": "25.2.1",
    "tslint": "5.20.1",
    "typescript": "3.7.5"
  },
  "dependencies": {
    "aws-sdk": "^2.218.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
